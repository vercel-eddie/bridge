{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/health", "dest": "/api/health.ts" },
    { "src": "/api/users/([^/]+)", "dest": "/api/users/[id].ts?id=$1" },
    { "src": "/api/users", "dest": "/api/users/index.ts" }
  ]
}
